{
  "name": "zum",
  "version": "0.1.11",
  "description": "Zum is a simple event bus for NodeJS microservices using amp.",
  "author": {
    "name": "Jonathan Brumley",
    "email": "cayasso@gmail.com"
  },
  "main": "./index.js",
  "scripts": {
    "test": "xo && mocha",
    "build": "npm run clean && ./node_modules/.bin/babel src -d lib",
    "prepare": "npm run build",
    "clean": "rm -rf lib/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cayasso/zum.git"
  },
  "dependencies": {
    "amp": "^0.3.1",
    "amp-message": "^0.1.2",
    "async": "^2.6.0",
    "debug": "^3.1.0",
    "eventemitter3": "^2.0.3",
    "netly": "^0.1.4",
    "tries": "^0.1.0",
    "wildcard-wrapper": "^0.1.2",
    "yeast": "^0.1.2"
  },
  "keywords": [
    "event",
    "bus",
    "broker",
    "microservices",
    "service",
    "communication",
    "event emitter",
    "emitter",
    "event bus",
    "process",
    "inter"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint-config-prettier": "^2.9.0",
    "mocha": "~4.0.1",
    "pre-commit": "^1.2.2",
    "should": "~13.1.3",
    "xo": "^0.18.2"
  },
  "pre-commit": [
    "test"
  ],
  "xo": {
    "extends": [
      "prettier"
    ],
    "ignores": [
      "test/**",
      "examples/**"
    ],
    "rules": {
      "object-curly-spacing": 0,
      "no-unused-expressions": 0,
      "no-negated-condition": 0,
      "no-use-before-define": 0,
      "new-cap": 0
    }
  }
}
